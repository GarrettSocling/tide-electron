{
  "name": "tide-electron",
  "version": "0.0.1",
  "description": "An Electron based IDE for TingBot",
  "author": {
    "name": "Tingbot Team",
    "email": "hello@tingbot.com",
    "url": "http://tingbot.com/"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron app/app.js",
    "postinstall": "install-app-deps",
    "pack": "webpack -p && build",
    "dist": "webpack -p && build",
    "dev": "concurrently --raw 'webpack --progress --watch' 'npm run start'"
  },
  "build": {
    "app-bundle-id": "your.id",
    "app-category-type": "your.app.category.type",
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    },
    "extraResources": [
      "default.tingapp"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tingbot/tide-electron.git"
  },
  "keywords": [
    "Electron",
    "TingBot"
  ],
  "bugs": {
    "url": "https://github.com/tingbot/tide-electron/issues"
  },
  "devDependencies": {
    "babel-core": "~6.8.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-runtime": "~6.8.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-runtime": "~6.6.1",
    "brace": "~0.8.0",
    "concurrently": "^2.1.0",
    "css-loader": "~0.23.1",
    "electron-builder": "^3.23.0",
    "electron-prebuilt": "~1.1.0",
    "file-loader": "~0.8.5",
    "font-awesome": "^4.6.1",
    "font-awesome-webpack": "0.0.4",
    "fs-extra": "^0.30.0",
    "jquery": "~2.2.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "~0.5.7",
    "vue": "~1.0.21",
    "vue-hot-reload-api": "~1.3.2",
    "vue-html-loader": "~1.2.2",
    "vue-loader": "~8.3.1",
    "vue-style-loader": "~1.0.0",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.1"
  },
  "directories": {
    "buildResources": "assets"
  }
}
