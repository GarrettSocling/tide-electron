{
  "name": "tide-electron",
  "version": "0.0.1",
  "description": "An Electron based IDE for TingBot",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "clean": "rm -rf ./build",
    "clean:lin": "rm -rf ./build/lin",
    "clean:osx": "rm -rf ./build/osx",
    "clean:win": "rm -rf ./build/win",
    "build": "npm run clean && npm run build:osx && npm run build:win && npm run build:lin",
    "build:lin": "npm run clean:lin && electron-packager ./ \"TideElectron\" --out=build/lin --platform=linux --arch=x64 --version=0.35.2 --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --ignore=build/",
    "build:osx": "npm run clean:osx && electron-packager ./ \"TideElectron\" --out=build/osx --platform=darwin --arch=x64 --version=0.35.2 --icon=assets/osx/tide.icns --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --ignore=build/",
    "build:win": "npm run clean:win && electron-packager ./ \"TideElectron\" --out=build/win --platform=win32 --arch=ia32 --version=0.35.2 --icon=assets/win/icon.ico --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --ignore=build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tingbot/tide-electron.git"
  },
  "keywords": [
    "Electron",
    "TingBot"
  ],
  "author": "rob4001",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/tingbot/tide-electron/issues"
  },
  "homepage": "https://github.com/tingbot/tide-electron#readme",
  "dependencies": {
    "jquery": "~2.2.3",
    "vue": "~1.0.21",
    "brace": "~0.8.0",
    "font-awesome-webpack": "0.0.4"
    "electron-default-menu": "^0.1.1",
  },
  "devDependencies": {
    "babel-core": "~6.7.7",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-runtime": "~6.7.5",
    "babel-preset-es2015": "~6.6.0",
    "babel-runtime": "~5.8.38",
    "css-loader": "~0.23.1",
    "electron-prebuilt": "~0.37.8",
    "file-loader": "~0.8.5",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "~0.5.7",
    "vue-hot-reload-api": "~1.3.2",
    "vue-html-loader": "~1.2.2",
    "vue-loader": "~8.2.3",
    "vue-style-loader": "~1.0.0",
    "watch": "^0.18.0",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.1"
  }
}
